{
  "extends": ["plugin:react/recommended","next/core-web-vitals"],
  "root": true,
  "plugins": ["react","unused-imports"],
  "rules": {
    "semi": [
      "warn",
      "always",
      {
        "omitLastInOneLineBlock": true
      }
    ],
    "react/jsx-uses-vars": "warn",
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error"
  }
}